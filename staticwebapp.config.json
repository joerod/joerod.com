{
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": [
      "/api/*",
      "/.auth/*",
      "/assets/*",
      "/*.css",
      "/*.js",
      "/*.png",
      "/*.jpg",
      "/*.jpeg",
      "/*.svg",
      "/*.ico",
      "/*.txt",
      "/*.json"
    ]
  },
  "routes": [
    {
      "route": "/admin",
      "rewrite": "/admin.html",
      "allowedRoles": [
        "admin"
      ]
    },
    {
      "route": "/admin.html",
      "allowedRoles": [
        "admin"
      ]
    },
    {
      "route": "/api/summary",
      "allowedRoles": [
        "admin"
      ]
    },
    {
      "route": "/api/locations",
      "allowedRoles": [
        "admin"
      ]
    },
    {
      "route": "/api/config",
      "allowedRoles": [
        "admin"
      ]
    }
  ],
  "responseOverrides": {
    "401": {
      "redirect": "/.auth/login/github",
      "statusCode": 302
    },
    "403": {
      "rewrite": "/403.html",
      "statusCode": 403
    }
  }
}
