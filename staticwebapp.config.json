{
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": [
      "/api/*",
      "/.auth/*",
      "/assets/*",
      "/*.css",
      "/*.js",
      "/*.png",
      "/*.jpg",
      "/*.jpeg",
      "/*.svg",
      "/*.ico",
      "/*.txt",
      "/*.json"
    ]
  },
  "routes": [
    { "route": "/api/admin-summary", "allowedRoles": ["admin"] },
    { "route": "/api/admin-locations", "allowedRoles": ["admin"] }
    { "route": "/admin", "rewrite": "/admin.html", "allowedRoles": ["admin"] },
    { "route": "/admin.html", "allowedRoles": ["admin"] },
    { "route": "/api/admin/*", "allowedRoles": ["admin"] }
  ],
  "responseOverrides": {
    "401": { "redirect": "/.auth/login/github", "statusCode": 302 },
    "403": { "rewrite": "/403.html", "statusCode": 403 }
  }
}
